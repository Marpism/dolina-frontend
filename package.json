{
  "name": "dolina_frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@pbe/react-yandex-maps": "^1.2.5",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "eslint": "^8.57.1",
    "prettier": "^3.8.1",
    "react": "^18.2.0",
    "react-beautiful-dnd": "^13.1.1",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-loading-skeleton": "^3.4.0",
    "react-responsive": "^9.0.2",
    "react-router-dom": "^6.21.0",
    "react-scripts": "5.0.1",
    "react-slick": "^0.30.2",
    "slick-carousel": "^1.8.1",
    "stylelint": "^17.3.0",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "cross-env REACT_APP_BACKEND_URL=https://dev.dolina.shop react-scripts start",
    "build": "cross-env REACT_APP_BACKEND_URL=https://backend.dolina.shop react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "check": "prettier --check './**/*.{html,css,js}'",
    "format": "prettier --write './**/*.{html,css,js}'",
    "stylelint": "stylelint **/*.css",
    "stylelintfix": "stylelint **/*.css --fix",
    "checkStyle": "npm run check && npm run stylelint",
    "fixStyle": "npm run format && npm run stylelintfix",
    "lint": "eslint src"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "cross-env": "^7.0.3"
  }
}
